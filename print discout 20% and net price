#include <iostream>
using namespace std;
int main()
{
    int qty , dis=0;
    float rate,expense;
    cout<<"enter quantity";
    cin>>qty;
    if(qty<=2000)
    
    dis=20;
    expense=(qty*rate)-(qty*rate)-(qty*rate*dis/100);
    cout<< "\n Total Expense =Rs."<< expense;
    return 0;
}
